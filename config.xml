<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.approx.guestapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>PraCasar</name>
    <description>
        Pra Casar.
    </description>
    <author email="contato@proprodutores.com.br" href="http://proprodutores.com.br">
        Aprox Tecnologia
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-compat" spec="git+https://github.com/apache/cordova-plugin-compat.git" />
    <plugin name="cordova-plugin-device" spec="^2.0.2" />
    <plugin name="cordova-plugin-fcm" spec="^2.1.2" />
    <plugin name="cordova-plugin-qrscanner" spec="^2.6.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <preference name="orientation" value="portrait" />
    <engine name="android" spec="^6.2.3" />
</widget>
